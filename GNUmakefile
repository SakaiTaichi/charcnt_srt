countchar : main.o QSort.o Swap.o ShowDataA.o ShowDataD.o help.o
	gcc main.o QSort.o Swap.o ShowDataA.o ShowDataD.o help.o -o countchar

main.o: main.c defs.h
	gcc -c main.c

QSort.o: QSort.c defs.h
	gcc -c QSort.c

Swap.o: Swap.c defs.h
	gcc -c Swap.c

ShowDataA.o: ShowDataA.c defs.h
	gcc -c ShowDataA.c

ShowDataD.o: ShowDataD.c defs.h
	gcc -c ShowDataD.c

help.o: help.c defs.h
	gcc -c help.c


